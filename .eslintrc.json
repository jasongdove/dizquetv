{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["@wildpeaks/eslint-config-commonjs", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["prettier"],
    "rules": {
        "require-jsdoc": 0,
        "prettier/prettier": ["error"],
        "max-statements": ["error", 1000]
    }
}
